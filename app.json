{
  "name": "Clean Air Council",
  "repository": "github.com/hack4impact/clean-air-council",
  "addons": [
    "heroku-postgresql",
    "redistogo"
  ],
  "env": {
    "FLASK_CONFIG": {
      "required": true
    },
    "SECRET_KEY": {
      "required": true
    },
    "MAIL_USERNAME": {
      "required": true
    },
    "MAIL_PASSWORD": {
      "required": true
    },
    "IMGUR_CLIENT_ID": {
      "required": true
    },
    "IMGUR_CLIENT_SECRET": {
      "required": true
    },
    "TWILIO_ACCOUNT_SID": {
      "required": true
    },
    "TWILIO_AUTH_TOKEN": {
      "required": true
    },
    "OPEN_WEATHER_MAP_API_KEY": {
      "required": true
    }
  }
}